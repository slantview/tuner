#!/bin/env ruby

require "tuner"

data = {
  "Header" => [
    "something",
    "something else"
    "other thing"
  ],
  "Next Header" => [
    "asdf asdf",
    "fdkjalsf sdfkjd",
    "asdfdf dfsdf"
    "asdf dfdkjdkjdlsk slkdj"  
  ],
  "Final Header" => [
    "This should be pretty long",
    "this should be devven longer because I dasid dso"
  ]
}

Tuner.ui.table(data)